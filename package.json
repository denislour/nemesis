{
  "name": "nemesis",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && swc ./src -d dist",
    "db:console": "docker-compose exec postgres psql -h localhost -U postgres -d nemesis",
    "start": "node dist/index.js",
    "start:docker": "yarn build && node --inspect=0.0.0.0 dist/index.js",
    "migrate": "yarn build && npx sequelize-cli db:migrate",
    "migrate:fresh": "yarn build && npx sequelize-cli db:migrate:undo:all && npx sequelize-cli db:migrate",
    "seed": "yarn build && npx sequelize-cli db:seed:all",
    "seed:fresh": "yarn build && npx sequelize-cli db:seed:undo:all && npx sequelize-cli db:seed:all"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "koa": "^2.13.4",
    "koa-morgan": "^1.0.1",
    "koa-router": "^12.0.0",
    "mysql2": "^2.3.3",
    "sequelize": "^6.24.0",
    "typescript": "^4.8.3"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.3",
    "@tsconfig/node16": "^1.0.3",
    "@types/dotenv": "^8.2.0",
    "@types/koa": "^2.13.5",
    "@types/koa-morgan": "^1.0.5",
    "@types/koa-router": "^7.4.4",
    "@types/node": "^18.7.23",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "eslint": "^8.24.0",
    "rimraf": "^3.0.2",
    "sequelize-cli": "^6.5.1"
  }
}
